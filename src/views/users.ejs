<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        *{
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        table{
            width: 60%;
            margin: 0 auto;
            margin-top: 200px;
            border-collapse: collapse;
            text-align: center;
            border: 1px solid black;
        }
        tr,td{
            border-top: 1px solid black;
        }
    </style>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Password</th>
                <th>Phone</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach((u) => {  %>
                <tr>
                    <td><%= u.id %></td>
                    <td><%= u.name %></td>
                    <td><%= u.email %></td>
                    <td><%= u.password %></td>
                    <td><%= u.phone %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>